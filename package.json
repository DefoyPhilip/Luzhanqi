{
  "name": "luzhanqi",
  "version": "1.0.0",
  "description": "Luzhanqi Board Game",
  "main": "dist/src/back-end/index.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./coverage,./test,./npm-debug.log --copy-files",
    "start-server": "npm run build && node dist/src/back-end/index.js",
    "lint": "node node_modules/eslint/bin/eslint.js -c .eslintrc ./src/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DefoyPhilip/Luzhanqi.git"
  },
  "author": "DefoyPhilip",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DefoyPhilip/Luzhanqi/issues"
  },
  "homepage": "https://github.com/DefoyPhilip/Luzhanqi#readme",
  "dependencies": {
    "express": "^4.16.4",
    "socket.io": "^2.2.0",
    "uniqid": "^5.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2"
  }
}
